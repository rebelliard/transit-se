{
  "name": "@transit-se/sdk",
  "version": "1.0.1",
  "description": "TypeScript SDK for Swedish public transit APIs (Trafiklab, GTFS-RT). Departures, stop lookups, service alerts, vehicle positions, and more.",
  "homepage": "https://github.com/rebelliard/transit-se/tree/main/packages/sdk#transit-sesdk",
  "type": "module",
  "license": "MIT",
  "author": "Rafael Belliard",
  "repository": {
    "type": "git",
    "url": "https://github.com/rebelliard/transit-se.git",
    "directory": "packages/sdk"
  },
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "prepublishOnly": "bun run build",
    "tc": "tsc --noEmit",
    "test": "bun test",
    "inspect": "set -a && . ../../.env 2>/dev/null; set +a; bun run dev/swagger.ts",
    "lint": "eslint src/",
    "version:patch": "bun ../../scripts/bump-version.ts patch",
    "version:minor": "bun ../../scripts/bump-version.ts minor",
    "version:major": "bun ../../scripts/bump-version.ts major",
    "version:pre": "bun ../../scripts/bump-version.ts prerelease"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "bun": "./src/index.ts",
      "import": "./dist/index.js"
    },
    "./sl": {
      "types": "./dist/api/sl/index.d.ts",
      "bun": "./src/api/sl/index.ts",
      "import": "./dist/api/sl/index.js"
    },
    "./trafiklab": {
      "types": "./dist/api/trafiklab/index.d.ts",
      "bun": "./src/api/trafiklab/index.ts",
      "import": "./dist/api/trafiklab/index.js"
    },
    "./gtfs": {
      "types": "./dist/api/gtfs/index.d.ts",
      "bun": "./src/api/gtfs/index.ts",
      "import": "./dist/api/gtfs/index.js"
    },
    "./combined": {
      "types": "./dist/api/combined/index.d.ts",
      "bun": "./src/api/combined/index.ts",
      "import": "./dist/api/combined/index.js"
    },
    "./schemas": {
      "types": "./dist/schemas/index.d.ts",
      "bun": "./src/schemas/index.ts",
      "import": "./dist/schemas/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "trafiklab",
    "gtfs",
    "gtfs-rt",
    "transit",
    "public-transport",
    "kollektivtrafik",
    "lokaltrafik",
    "sl",
    "stockholm",
    "sweden",
    "sverige"
  ],
  "peerDependencies": {
    "valibot": "^1.0"
  },
  "peerDependenciesMeta": {
    "valibot": {
      "optional": true
    }
  },
  "devDependencies": {
    "@hono/swagger-ui": "^0.5.3",
    "@types/bun": "^1.3.9",
    "hono": "^4.12.3",
    "valibot": "^1.2.0"
  },
  "dependencies": {
    "gtfs-realtime-bindings": "^1.1.1"
  }
}
